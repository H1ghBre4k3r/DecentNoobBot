{
    "name": "decentnoobbotredux",
    "version": "1.1.2",
    "description": "This is my typescript + redux version of the DecentNoobBot.",
    "main": "build/index.js",
    "scripts": {
        "build": "tsc",
        "start": "node .",
        "dev": "ts-node-dev ./src/index.ts",
        "build:start": "npm run build && npm run start",
        "docker:build": "docker-compose -f docker/docker-compose.yml build",
        "docker:deploy": "docker-compose -f docker/docker-compose.yml up -d",
        "docker": "npm run docker:build && npm run docker:deploy",
        "deploy": "npm run build && npm run docker"
    },
    "author": "Louis Meyer",
    "license": "ISC",
    "dependencies": {
        "discord.js": "^11.4.2",
        "request": "^2.88.0"
    },
    "devDependencies": {
        "@types/node": "^11.12.2",
        "@types/request": "^2.48.1",
        "ts-node": "^8.0.3",
        "ts-node-dev": "^1.0.0-pre.32",
        "typescript": "^3.4.1"
    }
}
